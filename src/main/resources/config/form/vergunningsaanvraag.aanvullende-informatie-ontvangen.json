            "collapsible": false,
            "key": "aanvraagInformatie",
            "type": "panel",
            "label": "Panel",
            "input": false,
            "tableView": false,
            "components": [
                {
                    "label": "Wat voor vergunning wilt u aanvragen?",
                    "widget": "choicesjs",
                    "tableView": true,
                    "data": {
                        "values": [
                            {
                                "label": "Parkeervergunning",
                                "value": "parkeervergunning"
                            }
                        ]
                    },
                    "validate": {
                        "required": true
                    },
                    "key": "aanvraag.typeAanvraag",
                    "type": "select",
                    "input": true
                },
                {
                    "label": "Per wanneer wilt u een vergunning aanvragen?",
